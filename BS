    loadstring(game:HttpGet("https://raw.githubusercontent.com/max2007killer/auto-build-not-limit/main/autobuild.txt"))()
end)
Section39:Button("无限方块", function()
    spawn(function()
        local guiinf = game:GetService("Players").LocalPlayer.PlayerGui.BuildGui.InventoryFrame.ScrollingFrame.BlocksFrame
        _G.Blocks = false
        while game:GetService("RunService").RenderStepped:wait() do
            if _G.Blocks then return end
            for i,v in pairs(guiinf:GetDescendants()) do
                if v.Name == "AmountText" then
                    v.Text = math.huge
                end
            end
        end
    end)
end)
Section39:Button("刷钱", function()
    loadstring(game:HttpGet(('https://raw.githubusercontent.com/urmomjklol69/GoldFarmBabft/main/GoldFarm.lua'),true))()
end)

